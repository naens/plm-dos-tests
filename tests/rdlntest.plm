rdlntest:
do;

$include (cons.ext)
$include (sys.ext)

    declare i word;
    declare buf (255) byte;

    call readln(.buf, 255);
    call prchr('{');
    call prstr(.buf);
    call prchr('}');
    call prcrlf;

    i = 0;
    do while buf(i) <> 0;
        call prhexbyte(buf(i));
        call prchr(':');
        call prchr(buf(i));
        call prcrlf;
        i = i + 1;
    end;

    call term;

end;
